<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.aoeng.huigu.model">

	<class name="Product" table="product">
		<id name="id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="name" column="name" />
		<property name="marketPrice" column="marketPrice" />
		<property name="pic" column="pic" />
		<property name="price" column="price" />
		<property name="limitPrice" column="limitprice" />
		<property name="leftTime" column="lefttime" />
		<property name="score" column="score" />
		<property name="available" column="available" />
		<property name="buyLimit" column="buylimit" />
		<property name="commentCount" column="commentcount" />
		<property name="inventoryArea" column="inventoryarea" />
		<property name="bigPic" column="bigpic" />
		<property name="sales" column="sales" />
		<property name="pubDate" column="pubdate" />
		<property name="topic" column="topic" />
		<property name="brand" column="brand"/>
		<property name="limitBuy" column="limitbuy"/>

	</class>
</hibernate-mapping>
