<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.aoeng.huigu.model">

	<class name="UserInfo" table="userinfo">
		<id name="id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="uid" column="uid"  />
		<property name="bonus" column="bonus" />
		<property name="level" column="level" />
		<property name="usersession" column="usersession" />
		<property name="ordercount" column="ordercount" />
		<property name="favoritescount" column="favoritescount" />

	</class>
</hibernate-mapping>
