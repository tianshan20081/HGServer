<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.aoeng.huigu.model">

	<class name="Address" table="address">
		<id name="id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="adsId" column="adsid" not-null="true" unique="true" />
		<property name="name" column="name" not-null="true" />
		<property name="phone" column="phone" not-null="true" />
		<property name="fixedTel" column="fixedtel" />
		<property name="provinceId" column="provinceid" not-null="true" />
		<property name="cityId" column="cityid" not-null="true" />
		<property name="areaId" column="areaid" not-null="true" />
		<property name="areaDetail" column="areadetail" not-null="true" />
		<property name="zipCode" column="zipcode" />

	</class>
</hibernate-mapping>
